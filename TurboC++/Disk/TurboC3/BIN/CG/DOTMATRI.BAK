void genH(int x,int y)
{
	int z = y,i,j;
	for(i=0;i<10;i++)
	{
		y = z;
		for(j=0;j<10;j++)
		{
			if(arrh[i][j]==1)
			{
				putpixel(x,y,1);
			}
			y++;
		}
		x++;
	}
}
void genE(int x,int y)
{
	int z = y,i,j;
	for(i=0;i<10;i++)
	{
		y = z;
		for(j=0;j<10;j++)
		{
			if(arre[i][j]==1)
			{
				putpixel(x,y,1);
			}
			y++;
		}
		x++;
	}
}
